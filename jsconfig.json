{
  "compilerOptions": {
    "checkJs": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "resolveJsonModule": true,
    "lib": ["es6", "dom", "es2017"],
    "target": "ES2015",
    "strict": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
    //  "paths": {
    //   "@hooks/*": ["hooks/*"],
    //   "@components/*": ["components/*"],
    //   "@utils/*": ["utils/*"],
    //   "@pages/*": ["pages/*"],
    // },
  },
  "include": ["src"],
  "exclude": ["node_modules", "build"]
}
